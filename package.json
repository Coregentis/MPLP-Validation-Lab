        "gate:pointer-integrity": "node scripts/gates/pointer-integrity-lab.mjs",
        "test:proof-signature-negative": "node scripts/tests/proof-signature-negative.mjs",
        "shadow:validate": "tsx scripts/shadow-validator.ts",
        "vlab:verify-proof": "tsx scripts/vlab-verify-proof.ts",
