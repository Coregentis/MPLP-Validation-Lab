        "gate:15": "tsx lib/gates/gate-15-curated-adjudication-required.ts",
        "vlab:recheck-hash": "tsx scripts/recheck-hash.ts",
        "vlab:recompute-pack-hashes": "tsx scripts/recompute-pack-hashes.ts",
        "generate:curated": "node scripts/generate-curated-runs.mjs",
        "gate:all": "npm run gate:vlab:00 && npm run gate:vlab:01",
