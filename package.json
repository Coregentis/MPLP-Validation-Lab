        "sync:invariants": "tsx scripts/sync-invariants.ts",
        "sync:all": "npm run sync:schemas && npm run sync:invariants",
        "gate:vlab:00": "tsx scripts/gates/verify-upstream-pin.ts",
        "gate:vlab:01": "tsx scripts/gates/verify-schema-alignment.ts",
        "gate:04": "tsx lib/gates/gate-04-language.ts",
