        "js-yaml": "^4.1.0",
        "lucide-react": "^0.562.0",
        "next": "^15.0.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-markdown": "^10.1.0",
        "tailwind-merge": "^3.4.0",
        "tailwindcss": "^4.1.18",
        "yaml": "^2.8.2"
    },
    "devDependencies": {
        "@playwright/test": "^1.57.0",
        "@tailwindcss/postcss": "^4.1.18",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^22",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "eslint": "^9",
        "eslint-config-next": "^15.0.0",
        "js-yaml": "^4.1.1",
        "postcss": "^8.5.6",
        "tsx": "^4.19.0",
        "typescript": "^5",
        "vitest": "^4.0.17"
    },
    "engines": {
        "node": ">=20"
    }
}